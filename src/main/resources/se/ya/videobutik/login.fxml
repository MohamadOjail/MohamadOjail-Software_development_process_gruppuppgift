    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Välj butik" />
      <ComboBox fx:id="cb_store" prefWidth="150.0" />
      <Label text="Välj medarbetare" />
      <ComboBox fx:id="cb_staff" prefWidth="150.0" />
      <Button fx:id="btn_ok" mnemonicParsing="false" text="OK" />
   </children>
</VBox>
