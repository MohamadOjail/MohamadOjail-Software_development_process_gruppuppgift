<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="20.0" stylesheets="@SectionStyle.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="40.0">
         <children>
            <VBox spacing="15.0">
               <children>
                  <Label id="title" text="Lägg till ny adress" />
                  <VBox>
                     <children>
                        <Label text="Gatuadress" />
                        <TextField fx:id="tf_add_first_name" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Län" />
                        <TextField fx:id="tf_add_last_name" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Postnummer" />
                        <TextField fx:id="tf_add_email" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Land" />
                        <ComboBox />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Postort" />
                        <ComboBox fx:id="cb_add_city" prefWidth="150.0" />
                     </children>
                  </VBox>
                  <Button id="section_btn" fx:id="btn_add_costumer" mnemonicParsing="false" text="Lägg till" />
               </children>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <VBox>
               <children>
                  <VBox spacing="15.0">
                     <children>
                        <Label id="title" text="Sök adress" />
                        <VBox>
                           <children>
                              <Label text="Sök efter gata" />
                              <TextField fx:id="tf_find_last_name" />
                           </children>
                        </VBox>
                        <TableView fx:id="tv_customers" prefHeight="200.0">
                           <columns>
                              <TableColumn minWidth="80.0" text="ID#" />
                              <TableColumn minWidth="150.0" prefWidth="150.0" text="Gata" />
                              <TableColumn minWidth="150.0" prefWidth="150.0" text="Stad" />
                              <TableColumn minWidth="150.0" prefWidth="150.0" text="Postnummer" />
                              <TableColumn minWidth="150.0" prefWidth="150.0" text="Land" />
                           </columns>
                        </TableView>
                        <Button id="section_btn" fx:id="btn_delete_costumer" mnemonicParsing="false" prefHeight="37.0" prefWidth="381.0" text="Radera adress" />
                     </children>
                  </VBox>
               </children>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <VBox spacing="15.0">
               <children>
                  <Label id="title" text="Redigera adress" />
                  <VBox>
                     <children>
                        <Label text="Gatuadress" />
                        <TextField fx:id="tf_add_first_name1" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Län" />
                        <TextField fx:id="tf_add_last_name1" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Postnummer" />
                        <TextField fx:id="tf_add_email1" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Land" />
                        <ComboBox />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Stad" />
                        <ComboBox fx:id="cb_add_city1" prefWidth="150.0" />
                     </children>
                  </VBox>
                  <Button id="section_btn" fx:id="btn_add_costumer1" mnemonicParsing="false" text="Uppdatera" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
</VBox>
